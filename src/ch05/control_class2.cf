body common control
{
      bundlesequence => { "test" };
}

bundle common g
{
  vars:
      "class_file" string => "/var/cfengine/site/classes.txt";
      "class_strs" slist => readstringlist("$(class_file)", "#.*?$", "\s+", "inf", "inf");
  classes:
      "$(class_strs)"  expression => "any";
}

bundle agent test
{
  reports:
    no_restarts::
      "no_restarts is defined";
    foobar::
      "foobar is defined";
}
